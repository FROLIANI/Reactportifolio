{
  "expo": {
    "name": "portifolio",
    "slug": "portifolio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/portifolio.jpg",
    "portifolio": {
      "image": "./assets/portifolio.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "portifolio": "./assets/portifolio.jpg"
    }
  }
}
